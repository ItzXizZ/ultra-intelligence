{
  "name": "ultra-intelligence-chatbot",
  "version": "1.0.0",
  "description": "Terminal-based chatbot system with dual AI models for conversation and data extraction",
  "main": "server.js",
  "scripts": {
    "start": "node --no-deprecation server.js",
    "dev": "nodemon --no-deprecation server.js",
    "setup-db": "node --no-deprecation setup-database.js",
    "test": "node --no-deprecation test-system.js",
    "ui": "cd ui && npm start",
    "ui-build": "cd ui && npm run build",
    "ui-install": "cd ui && npm install",
    "server": "node server.js",
    "server-dev": "nodemon server.js",
    "dev-full": "concurrently \"npm run server-dev\" \"npm run ui\"",
    "start-ui": "concurrently \"npm run server\" \"npm run ui\"",
    "build": "cd frontend && npm install && npm run build",
    "vercel-build": "cd frontend && npm install && npm run build"
  },
  "dependencies": {
    "openai": "^4.20.1",
    "readline": "^1.3.0",
    "dotenv": "^16.3.1",
    "pg": "^8.11.3",
    "@supabase/supabase-js": "^2.38.4",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "concurrently": "^8.2.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "chatbot",
    "ai",
    "terminal",
    "student-counseling",
    "database"
  ],
  "author": "UltraIntelligence",
  "license": "MIT"
}
